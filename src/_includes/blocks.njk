<div class="container">
    <div class="columns">
        {% for key, val in blocks %}
            <div class="column">
                <div class="card">                        
                    <div class="card-content">
                        <div class="content">
                            <strong {% if key == "block_d" %} style="color:green" {% endif %}>{{ val.name }}</strong>
                        </div>
                    </div>
                    <footer>
                        <a href="{{ val.ref }}" class="card-footer-item">Continue reading</a>
                    </footer>
                </div>                
            </div>            
        {% endfor %}
    </div>
</div>